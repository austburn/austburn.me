[
  {
    "name": "web",
    "image": "${repository}:${git_hash}",
    "cpu": 10,
    "memory": 100,
    "essential": true,
    "networkMode": "host",
    "environment": [
      { "name" : "GIT_HASH", "value": "${git_hash}" }
    ],
    "portMappings": [
      {
        "containerPort": 5050,
        "hostPort": 5050
      }
    ]
  }
]
